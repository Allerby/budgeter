<div class={{routeStyleNamespaceClassSet}}>
  {{#if (eq environment 'development')}}
    <AnimatedTools />
  {{/if}}

  {{#if (or 
      (eq router.currentRouteName 'login') 
      (eq router.currentRouteName 'register')
      (eq router.currentRouteName 'forgot-password')
    )
  }}
    <PrimaryNavigation />
  {{/if}}

  {{outlet}}

  {{#if (eq router.currentRouteName 'index')}}
    {{set-body-class 'bg-primary-4 overflow-hidden'}}
    <img alt='logo' class='w-12 absolute pin-t pin-l mx-auto mt-12 ml-12' src='/logo-only.png'>
    {{body-movin path='background_data' class='w-screen'}}
    {{body-movin path='lady_data' id='space_lady' class='absolute height-90 pin-t'}}
  {{/if}}

</div>
